<Application x:Class="Syncris.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Syncris"
             ShutdownMode="OnExplicitShutdown" Exit="Application_Exit" Startup="Application_Startup">
    <Application.Resources>

        <SolidColorBrush x:Key="ErrorCellBankgroundColor" Color="#FFFF6969"/>
        <SolidColorBrush x:Key="ValidCellBackgroundColor" Color="#FF8BFF69"/>
        <Image x:Key="Icon" Source="/Resources/SyncrisIcon.ico" />
    </Application.Resources>
</Application>
